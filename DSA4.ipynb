   "source": [
    "def FirstNonRepeat(s):\n",
    " \n",
    "    for i in s:\n",
    " \n",
    "        if (s.find(i, (s.find(i)+1))) == -1:\n",
    " \n",
    "            print(\"First non-repeating character is\", i)\n",
    " \n",
    "            break\n",
    " \n",
    "    return\n",
    " \n",
    " \n",
    "s = 'Edyoda'\n",
    " \n",
    "FirstNonRepeat(s)"
   ]
